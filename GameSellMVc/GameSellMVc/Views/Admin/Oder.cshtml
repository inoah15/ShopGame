
@{
    ViewBag.Title = "Oder";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}
<link rel="stylesheet" type="text/css" media="screen" href="~/Asset/admin/style/orders.css" />
<script src="~/Asset/admin/script/login.js"></script>
<div class="body">
    <div class="list">
        <a class="list-box" @Html.ActionLink("Dashboard", "DashboardGame", "Admin") </a>
        <a class="list-box" @Html.ActionLink("Add Product", "AddGame", "Admin") </a>
        <a class="list-box" @Html.ActionLink("Customer", "Customer", "Admin") </a>
        <a class="list-box" @Html.ActionLink("Remove/Edit Product", "RemoveGame", "Admin") </a>
        <a class="list-box" @Html.ActionLink("Orders", "Oder", "Admin") </a>
    </div>
    <div class="info">
        <div class="remove-product-title">
            All Orders
        </div>
        <div class="table-wrapper">
            <iframe src="~/Asset/admin/list/all-orders-game.html" frameBorder="0" border-radius="17px" height="100%" width="100%"></iframe>
        </div>
    </div>
</div>

